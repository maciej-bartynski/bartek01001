{
  "name": "@bartek0x1001/crud",
  "version": "1.0.0",
  "description": "Automated CRUD endpoints for JSON file storage - supports both Express.js and bare Node.js servers",
  "main": "dist/index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "crud",
    "express",
    "nodejs",
    "bare-nodejs",
    "json",
    "file-storage",
    "api",
    "rest",
    "dual-mode"
  ],
  "author": "bartek0x1001",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maciej-bartynski/crud.git"
  },
  "bugs": {
    "url": "https://github.com/maciej-bartynski/crud/issues"
  },
  "homepage": "https://github.com/maciej-bartynski/crud#readme",
  "imports": {
    "#src/*.js": "./dist/*.js",
    "#src/**/*.js": "./dist/**/*.js"
  },
  "scripts": {
    "dev": "concurrently \"tsc --watch --pretty\" \"vitest --watch\"",
    "test": "vitest --run --reporter=verbose --silent=false",
    "build": "tsc --build --force --pretty"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.3"
  },
  "dependencies": {
    "@bartek0x1001/async-tasks-queue": "^1.0.0",
    "@bartek0x1001/json-storage": "^1.0.0",
    "uuid": "^11.1.0"
  },
  "peerDependencies": {
    "express": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  }
}